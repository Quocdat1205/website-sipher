        "build-gallery-page": "cd web-gallery && yarn build",
        "install-stacking-page": "yarn build-components && cd web-staking && yarn",
        "build-stacking-page": "cd web-staking && yarn build",
        "install-ido-page": "yarn build-components && cd web-ido && yarn",
        "build-ido-page": "cd web-ido && yarn build"
